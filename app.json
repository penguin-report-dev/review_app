{
  "expo": {
    "name": "Penguin Report",
    "slug": "Penguin-Report",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.penguinreport.expo.googlesignin",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.542840505079-3637lts4m6pjovuptfcjso9urhvk2rfv"
        }
      }
    },
    "android": {
      "package": "com.penguinreport.expo.googlesignin",
      "googleServicesFile": "./google-services.json"
    }
  }
}